# Server Configuration
PORT=3001
NODE_ENV=development

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Cashfree Configuration
CASHFREE_APP_ID=your_cashfree_test_app_id
CASHFREE_SECRET_KEY=your_cashfree_test_secret_key
CASHFREE_ENV=TEST
# Options: TEST, PRODUCTION

# Production Cashfree (optional)
CASHFREE_APP_ID_PROD=your_cashfree_prod_app_id
CASHFREE_SECRET_KEY_PROD=your_cashfree_prod_secret_key

# CORS Configuration
FRONTEND_URL=http://localhost:5173
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000,https://your-frontend-domain.com

# Security
JWT_SECRET=your_jwt_secret_key
API_RATE_LIMIT=100

# Logging
LOG_LEVEL=info